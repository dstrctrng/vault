#!/bin/bash -e

pth_install=$1; shift

# cache software
rsync -ia --no-t vault/software/. $pth_install/software/ --delete

# cache downloads, isos
rsync -ia --no-t vault/virtualbox/. $pth_install/virtualbox/ --delete

# cache distfiles
rsync -ia --no-t vault/macports/. $pth_install/macports/ --delete
